class Calculator extends Something {

	// centigrade to Farenheit conversion
    // F = 9 * C / 5 + 32
	public int c2f(int local_1) {
		local_1 = 9 * local_1;
		local_1 = local_1/5;
		local_1 = local_1 + 32;
		return local_1;
	}


	// Farenheit to centigrade conversion
	// C = 5 * (f - 32) / 9
	public int f2c(int local_1) {
		local_1 = local_1 - 32;
		local_1 = local_1 * 5;
		local_1 = local_1 / 9;
		return local_1;
	}
}